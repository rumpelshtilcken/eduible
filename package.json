{
  "name": "eduible",
  "version": "0.0.1",
  "scripts": {
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint \"./app/{pages,components}/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "commitmsg": "validate-commit-msg",
    "precommit": "lint-staged && npm test",
    "prepush": "npm test",
    "dev": "nodemon server/index.js --config .nodemonrc",
    "build": "npm run clean && next build app -p $PORT && babel server -d dist",
    "clean": "rimraf dist && mkdir dist",
    "start": "NODE_ENV=production npm run build && node dist",
    "heroku-postbuild": "npm run build",
    "test": "NODE_ENV=test mocha --opts .mocharc"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "lint-staged": {
    "{pages,components}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "auth0-js": "^8.10.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bowser": "^1.7.3",
    "btoa": "^1.1.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "email-validator": "^1.1.1",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "graphql": "^0.10.3",
    "graphql-sequelize": "^5.4.2",
    "graphql-server-express": "^1.1.2",
    "graphql-tools": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^7.4.3",
    "jssha": "^2.3.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.2",
    "moment": "^2.18.1",
    "next": "^3.0.6",
    "nodemailer": "^4.0.1",
    "normalize-css": "^2.3.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.1.2",
    "pg-hstore": "^2.3.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "rc-slider": "^8.3.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-chat-ui": "^0.1.0",
    "react-circular-progressbar": "^0.2.1",
    "react-country-region-selector": "^1.2.1",
    "react-custom-scrollbars": "^4.1.2",
    "react-datepicker": "^0.55.0",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.4",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.6",
    "react-style-proptype": "^3.0.0",
    "react-tabs": "^1.1.0",
