{
  "name": "eduible",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://ak8912_@bitbucket.org/eduible/eduible-web.git",
  "author": "intellection",
  "scripts": {
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint \"{pages,components}/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "test": "npm run lint",
    "dev": "next",
    "commitmsg": "validate-commit-msg",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "build": "next build",
    "start": "next start"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "lint-staged": {
    "{pages,components}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "max-empty-lines": 2,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "express": "^4.15.3",
    "next": "^2.4.5",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.1",
    "lint-staged": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^3.0.0"
  }
}
