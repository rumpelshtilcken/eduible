{
  "name": "eduible",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://ak8912_@bitbucket.org/eduible/eduible-web.git",
  "author": "intellection",
  "scripts": {
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint \"{pages,components}/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "test": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "dev": "sequelize db:migrate && NODE_ENV=development PORT=3000 node server/index.js",
    "build": "next build",
    "start": "node server/index.js",
    "now": "now"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "lint-staged": {
    "{pages,components}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "max-empty-lines": 2,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "email-validator": "^1.0.7",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "global": "^4.3.2",
    "isomorphic-fetch": "^2.2.1",
    "lusca": "^1.4.1",
    "next": "^2.4.5",
    "next-redux-wrapper": "^1.1.3",
    "nodemailer": "^4.0.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "normalize.css": "^7.0.0",
    "now": "^7.2.4",
    "now-logs": "^0.0.7",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-analog-clock": "^1.6.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-modal": "^2.2.2",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.3.2",
    "sequelize-cli": "^2.7.0",
    "session-file-store": "^1.0.0",
    "simple-react-modal": "^0.5.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.1",
    "lint-staged": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^3.0.0"
  }
}
